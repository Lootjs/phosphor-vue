{
  "name": "phosphor-vue",
  "version": "1.4.0",
  "description": "A clean and friendly icon family for Vue, too!",
  "author": "rektdeckard",
  "license": "MIT",
  "repository": "phosphor-icons/phosphor-vue",
  "homepage": "https://phosphoricons.com",
  "sideEffects": false,
  "main": "dist/phosphor-vue.es.js",
  "browser": "dist/phosphor-vue.es.js",
  "module": "dist/phosphor-vue.es.js",
  "unpkg": "dist/phosphor-vue.umd.js",
  "jsdelivr": "dist/phosphor-vue.umd.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "icons",
    "svg",
    "phosphor",
    "design",
    "interface",
    "UI",
    "UX"
  ],
  "scripts": {
    "serve": "vite",
    "prebuild": "rimraf ./dist",
    "build:vite": "vite build && vue-tsc --emitDeclarationOnly",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "analyze": "vite build --config analyze.config.js",
    "assemble": "node bin/assemble.js",
    "postbuild": "cpy ./src/types.d.ts ./dist",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,vue}\" \"./build/**/*.{js,ts}\" \"./bin/**/*.{js,ts}\""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@types/node": "^17.0.13",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.29",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.4",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-vue2": "^1.9.3",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.31.1"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
